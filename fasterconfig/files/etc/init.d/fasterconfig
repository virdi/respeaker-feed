#!/bin/sh /etc/rc.common
# Copyright (C) 2016 Seeed Technology Limited.

# start after and stop before networking
START=80
STOP=89

USE_PROCD=1

service_triggers()
{
	procd_add_reload_trigger "fasterconfig"
}

start_service()
{
	procd_open_instance
	procd_set_param command fasterconfig
	procd_set_param respawn
	procd_close_instance
}
